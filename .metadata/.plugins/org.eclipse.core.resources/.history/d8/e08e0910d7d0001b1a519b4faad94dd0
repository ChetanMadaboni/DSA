package com.wiley.bank;

import java.util.ArrayList;
import java.util.Scanner;
//1,2,3,4
public class Exam {
	public static void main(String []args) {
		Scanner sc=new Scanner(System.in);
		ArrayList<Integer> arr=new ArrayList<Integer>();
		while(sc.hasNextInt()) {
			arr.add(sc.nextInt());
		}
	}
}
